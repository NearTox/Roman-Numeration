<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <MyOutDir>$(SolutionDir)bin\$(Configuration)-$(PlatformTarget)\</MyOutDir>
    <MyTempDir>$(SolutionDir)obj\$(ProjectName)\$(Configuration)-$(PlatformTarget)-vs$(VisualStudioVersion)\</MyTempDir>
    <MyLibDir>$(MyOutDir)</MyLibDir>
    <MyGTest>$(SolutionDir)googletest\googletest\include;$(SolutionDir)googletest\googletest\;</MyGTest>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="MyOutDir">
      <Value>$(MyOutDir)</Value>
    </BuildMacro>
    <BuildMacro Include="MyTempDir">
      <Value>$(MyTempDir)</Value>
    </BuildMacro>
    <BuildMacro Include="MyLibDir">
      <Value>$(MyLibDir)</Value>
    </BuildMacro>
    <BuildMacro Include="MyGTest">
      <Value>$(MyGTest)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>